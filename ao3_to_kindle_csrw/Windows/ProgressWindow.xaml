<Window x:Class="AO3EbookDownloader.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AO3EbookDownloader"
        mc:Ignorable="d"
        Title="Please wait..." Height="200" Width="400">
    <Grid>
        <Grid x:Name="gridInnerProgress">
            <Label x:Name="labelProgressBar" Content="Building library..." Margin="91,24,91,119" HorizontalContentAlignment="Center"/>
            <ProgressBar x:Name="progressBar" Margin="10,55,10,0" Height="43" VerticalAlignment="Top" IsTabStop="False"/>
            <Button x:Name="buttonCancelProgress" Height="25" Width="140" Content="Stop" Margin="126,103,126,41" />
        </Grid>
    </Grid>
</Window>
